                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:hasDocumentType ;
                            owl:cardinality "1"^^xsd:nonNegativeInteger                      
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:manuscriptHasDateReadable ;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger       
                          ],
                          [
                            rdf:type owl:Restriction ;
                            owl:onProperty roud-oeuvres:manuscriptHasDateComputable ;
                            owl:minCardinality "0"^^xsd:nonNegativeInteger       
