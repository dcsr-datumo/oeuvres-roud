               <namespace>noNamespace</namespace>
               <propertyIri>http://www.knora.org/ontology/0112/roud-oeuvres#standoffNoteHasResp</propertyIri>
            </attribute>
         </attributes>
      </standoffClass>
   </mappingElement>

   
   <mappingElement>
      <tag>
         <name>TEI</name>
         <class>noClass</class>
         <namespace>noNamespace</namespace>
         <separatesWords>false</separatesWords>
