#!/usr/bin/expect

set timeout 120
spawn console.sh --force --echo --serverURL [lindex $argv 0]
set prompt "> "
expect $prompt
send "open knora-test .\r"


expect $prompt
send "load ./roudMapping.ttl into http://www.knora.org/ontology/0112/roud-oeuvres .\r"


send "close .\r"
expect $prompt
send "exit .\r"